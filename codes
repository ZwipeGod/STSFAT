==================================================================
HOURGLASS
import java.util.Scanner;
public class Main {
    public static int findMaxSum(int [][]mat, int R, int C) {
        int max_sum = 0;
        int sum;
        if (R < 3 || C < 3) {
            System.out.println("Not possible");
            System.exit(0);
        }
        for (int i = 0; i < R - 2; i++) {
            for (int j = 0; j < C - 2; j++) {
                sum = (mat[i][j] + mat[i][j + 1] + mat[i][j + 2]) // Top row
                      + (mat[i + 1][j + 1])                     // Middle element
                      + (mat[i + 2][j] + mat[i + 2][j + 1] + mat[i + 2][j + 2]); // Bottom row
                max_sum = Math.max(max_sum, sum);
            }
        }
        return max_sum;
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int R = sc.nextInt();
        int C = sc.nextInt();
        int[][] mat = new int[R][C];
        for (int i = 0; i < R; i++) {
            for (int j = 0; j < C; j++) {
                mat[i][j] = sc.nextInt();
            }
        }
        System.out.println("Maximum hourglass sum: " + findMaxSum(mat, R, C));
    }
}
