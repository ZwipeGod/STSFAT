BLOCK SWAP
import java.util.*;
public class Main {
    public static void main(String[] args) {
        int n;
        int k;
        Scanner sc = new Scanner(System.in);
        n = sc.nextInt();
        int arr[] = new int[n];
        for(int i = 0; i < n; i++) {
            arr[i]=sc.nextInt();
        }
        k = sc.nextInt();
        k = k%n;
        int temp[]=new int[k];
        System.arraycopy(arr,0,temp,0,k);
        System.arraycopy(arr,k,arr,0,n-k);
        System.arraycopy(temp,0,arr,n-k,k);
        for(int i = 0;i<n;i++){
            System.out.print(arr[i]+" ");
        }
        }
}
