GCD
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        int n1, n2;
        Scanner sc = new Scanner(System.in);
        n1 = sc.nextInt();
        n2 = sc.nextInt();
        if (n1 < n2) {
            int temp = n1;
            n1 = n2;
            n2 = temp;
        }
        int result = gcd(n1,n2);
        System.out.println("The GCD of " + n1 + " and " + n2 + " is: " + result);
    }

    public static int gcd(int n1, int n2) {
        if(n1 == 0){
            return  n2;
        }
        return gcd(n2%n1,n1);
        
    }
}
