import java.util.*;
public class Main {
    public static void main(String args[]){
        Scanner scan = new Scanner(System.in);
        int n = scan.nextInt();
        int swapnum = ((n & 0x0F) <<4 | (n & 0xF0)>>4);
        System.out.println("Number before swap: "+n);
        System.out.println("Number after swap: "+swapnum);
    }
}
